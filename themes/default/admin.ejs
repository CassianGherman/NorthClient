                    <script>
                        fetch("https://raw.githubusercontent.com/NorthDevelopment/NorthClient/main/assets/lv.json").then(res => Promise.resolve(res.json()).then(lv => {
                            let version = "<%= settings.version %>"
                            if (version == lv.version) {
                                document.getElementById("latestversion").innerHTML = `<span>You are running NorthClient version <code style="color: #007fcc" >v${version}</code>. NorthClient is up-to-date!</span>`;
                            } else {
                                document.getElementById("latestversion").innerHTML = `<span>NorthClient is <b>not up-to-date!</b> The latest version is <code style="color: #007fcc" >v${lv.version}</code> and you are currently running version <code style="color: #007fcc" >v<%= settings.version %></code>.</span>`;
                            }
